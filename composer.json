{
    "name": "rhapsody-project/security-bundle",
    "type": "symfony-bundle",
    "description": "Rhapsody Security Bundle",
    "keywords": ["security", "JWT"],
    "homepage": "https://github.com/rhapsody-project/RhapsodySecurityBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Quinn",
            "email": "sean.quinn@extesla.com",
            "homepage": "https://github.com/swquinn"
        },
        {
            "name": "contributors",
            "homepage": "https://github.com/rhapsody-project/RhapsodySecurityBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.5|^7.0",
        "doctrine/mongodb": ">=1.0.1",
        "symfony/framework-bundle": "^2.8|^3.0",
        "symfony/security-bundle": "^2.8|^3.0",
        "symfony/console": "^2.8|^3.0",
        "lcobucci/jwt": "~3.2",
        "namshi/jose": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.0",
        "symfony/phpunit-bridge": "^2.8|^3.0",
        "symfony/browser-kit": "^2.8|^3.0",
        "symfony/dom-crawler": "^2.8|^3.0",
        "symfony/yaml": "^2.8|^3.0",
        "friendsofphp/php-cs-fixer": "^1.1"
    },
    "suggest": {
        "lcobucci/jwt": "For using the LcobucciJWTEncoder"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Rhapsody\\SecurityBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "target-dir": "Rhapsody/SecurityBundle"
}
